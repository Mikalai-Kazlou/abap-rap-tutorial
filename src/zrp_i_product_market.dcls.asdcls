@EndUserText.label: 'Product Market'
@MappingRole: true
define role ZRP_I_PRODUCT_MARKET {
  grant 
    select
      on
        ZRP_I_PRODUCT_MARKET
          where
            ( ISOCode ) = aspect pfcg_auth( ZRPMRKTISO, ZRPISOCODE, ACTVT = '03' ) 
           or CreatedBy = $session.user;
            
}